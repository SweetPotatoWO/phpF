@charset "utf-8";

*{ margin:0; padding:0;}
html{-webkit-text-size-adjust:none;-webkit-user-select:none;}
body{ background:#F2F4FA;font-family:"Microsoft YaHei","微软雅黑",Helvetica,Arial,sans-serif;color:#353B51;}
ol,ul{padding-left:0;list-style-type:none; list-style:none;}
table{border-collapse:collapse;border-spacing:0;}
select {border: none;background: none;width: 100%;appearance:none;-moz-appearance:none;-webkit-appearance:none;}
th,dt{font-style:normal;font-weight:normal;}
h1,h2,h3,h4,h5,h6,b{font-weight:normal;}
em,i{font-style:normal;}
a {color: #353B51;}
a:hover,a:focus,a:active {color:initial;text-decoration: none;}

.fl {float: left;}
.fr {float: right;}
.block {display: block;}
.none {display: none;}
.inline-block {display: inline-block;}
.relative {position: relative;}
.absolute {position: absolute;}
.fixed {position: fixed}
.overflow {overflow: hidden;}
.text-ellipsis {overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.clearfix:before,.clearfix:after {content: "";display: table;line-height: 0;}

/*weui重置*/
.weui-toast_content {margin-bottom: 0 !important;}
.weui-toast--text {font-size: 1rem !important;}

/*font*/
.fs-10 {font-size: .71rem;}
.fs-12 {font-size: .86rem;}
.fs-13 {font-size: .93rem;}
.fs-15 {font-size: 1.07rem;}
.fs-14 {font-size: 1rem;}
.fs-16 {font-size: 1.14rem;}
.fs-18 {font-size: 1.28rem;}
.fs-24 {font-size: 1.71rem;}
.fw-b {font-weight: bold;}
.color-content {color: #343B53;}
.color-blue {color: #477BFF;}
.color-orange {color: #FF5240;}
.color-light {color: #DFE1E6;}
.color-deep {color: #94969E;}
.color-7 {color: #777777;}
.color-white {color: #ffffff;}
.text-ellipsis {white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}


/*border*/
.bord {border: 1px solid #F2F4FA;}
.bord-n {border: none !important;}
.bord-t {border-top: 1px solid #F2F4FA;}
.bord-b {border-bottom: 1px solid #F2F4FA;}
.bord-l {border-left: 1px solid #F2F4FA;}
.bord-r {border-right: 1px solid #F2F4FA;}
.bord-t-n { border-top:none !important;}
.bord-b-n { border-bottom:none !important;}
.bord-l-n { border-left:none !important;}
.bord-r-n { border-right:none !important;}

/*通用边距*/
.pad-0 {padding: 0;}
.pad-10 {padding: .714rem;}
.pad-15 {padding: 1.071rem;}
.pl-5 {padding-left: .357rem !important;}
.pl-10 {padding-left: .714rem;}
.pl-15 {padding-left: 1.071rem;}
.pl-20 {padding-left: 1.428rem;}
.pl-25 {padding-left: 1.78rem;}
.pl-30 {padding-left: 2.142rem;}
.pr-10 {padding-right: .714rem;}
.pr-15 {padding-right: 1.071rem;}
.pr-25 {padding-right: 1.78rem;}
.pt-10 {padding-top: .714rem;}
.pt-15 {padding-top: 1.071rem;}
.pt-20 {padding-top: 1.428rem;}
.pt-30 {padding-top: 2.14rem;}
.pt-50 {padding-top: 3.57rem;}
.pb-5 {padding-bottom: .357rem !important;}
.pb-10 {padding-bottom: .714rem;}
.pb-15 {padding-bottom:1.071rem;}
.mt-10 {margin-top: .714rem;}
.mr-15 {margin-right: 1.071rem;}
.mb-5 {margin-bottom: .357rem !important;}
.mb-10 {margin-bottom: .714rem;}
.mb-15 {margin-bottom: 1.071rem;}
.mb-20 {margin-bottom: 1.428rem;}

/*background*/
.bg-white {background: #fff;}

/*tab*/
.nav {margin: .714rem auto;}
.nav li a {display: block;height: 3.57rem;line-height: 3.57rem;color: #343B53;}
.nav li a.active {color:#357CFF;border-bottom: 2px solid #357CFF;}

/*index*/
.index .section-ct {width: 100%;height: 100%;}
.index .section-ct .next {width:100%;bottom:1%;}
.index .section-ct .next a {display: block;width: 1.82rem;height: 1.64rem;background: url(/Public/wechat/images/index/arrow.png) no-repeat;background-size: 100% 100%;margin: 0 auto;-webkit-animation: jump 1.2s linear infinite;animation: jump 1.2s linear infinite;}
.index .section-ct-1 {background: url(/Public/wechat/images/index/page-01.png) no-repeat center bottom;background-size: 100% 47%;}
.index .section-ct-1 .toPc {background: #ffffff;border-radius: .35rem;width: 4.5rem;height: 1.57rem;margin-top: 1.21rem;}
.index .section-ct-1 .logo img {width: 7rem;margin-top: 12%;box-shadow: 0 5px 14px rgba(57,99,194,.56);border-radius: 1.8rem;}
@media only screen and (max-width: 320px) and (max-height:480px) {
	.index .section-ct-1 .logo img {margin-top: 6%;}
}
.index .section-ct-1 .download {display:block;width: 12.07rem;height: 3.1rem;line-height: 3.1rem;color: #477BFF;font-weight: bold;background:#fff;border-radius: .35rem;margin: 0 auto;}
.index .section-ct-1 h1 {font-size: 1.78rem;margin: 2.7rem 0 1.4rem;}
.index .section-ct-2 {background: url(/Public/wechat/images/index/page-02.png) no-repeat center bottom;background-size: 100% 100%;}
.index .section-ct-2 .clearfix {width: 4.5rem;margin: 0 auto;padding-top: 14.428rem;}
.index .section-ct-2 .clearfix .fl {font-size: 1.78rem;width:  1.78rem;line-height: 1.3;}
.index .section-ct-2 .clearfix .fr {font-size: 1.07rem;width:  1.07rem;line-height: 1.3;padding-top: 3.6rem;}
.index .section-ct-3 {background: url(/Public/wechat/images/index/page-03.png) no-repeat center bottom;background-size: 100% 100%;}
.index .section-ct-4 {background: url(/Public/wechat/images/index/page-04.png) no-repeat center bottom;background-size: 100% 100%;}
.index .section-ct-5 {background: url(/Public/wechat/images/index/page-05.png) no-repeat center bottom;background-size: 100% 100%;}
.index .section .section-ct.section-ct-5 .next {bottom: 2%;}
.index .section .section-ct.section-ct-5 .next a {width: 6.5rem;height: 2.21rem;line-height: 2.21rem;background: #ffffff;color: #477BFF;font-weight: bold;text-align: center;border-radius: .35rem;-webkit-animation: none;animation: none;}
.index .section-ct .title {margin-top: 0;padding: 8.71rem 0 2rem;font-size: 1.78rem;}
.index .section-ct .des {font-size: 1.07rem;}
@keyframes jump{
    0%{
        transform:translate(0,0px);
        -webkit-transform:translate(0,0px);
    }
    25%{
        transform:translate(0,-3px);
        -webkit-transform:translate(0,-3px);
    }
    50%{
        transform:translate(0,1px);
        -webkit-transform:translate(0,1px);
    }
    75%{
        transform:translate(0,-1px);
        -webkit-transform:translate(0,-1px);
    }
    100%{
        transform:translate(0,0px);
        -webkit-transform:translate(0,0px);
    }
}
/*个人主页*/
.personal .download {padding: .7857rem 1.21rem;background: #E9EFFF;}
.personal .download .fl {height:1.857rem;line-height:1.857rem;background: url(/Public/wechat/images/user/logo.png) no-repeat 0 center;background-size: 1.857rem 1.857rem;padding-left: 2.4rem;}
.personal .download .fr {padding-top: .16rem;}
.personal .download .fr a {display:block;width:3.57rem;height:1.6428rem;line-height:1.6428rem;text-align:center;background: #477BFF;color: #fff;border-radius: .214rem;}
.personal .tag {width: 100%;min-height: 12rem;background: #fff url(/Public/wechat/images/user/bg.png) center top no-repeat;background-size: 100% 5.57rem;padding-top: 3rem;}
.personal .tag .info {width: 90.9333%;height: 7.928rem;background: #fff;box-shadow: 0 2px 18px rgba(91,91,91,.35);margin: 0 auto;border-radius: .357rem;padding: 1.5rem 0 0 1.07rem;}
.personal .tag .info .userInfo {width: 76%;}
.personal .tag .info .userInfo .company-name {overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.personal .tag .info .portrait {width: 4.28rem;height: 4.28rem;}
.personal .tag .info .p,.personal .tag .info .v {width: 1.07rem;height: .928rem;vertical-align: middle;}
.personal .tags {padding: .5rem 1.21rem 0;margin-top: 1rem;}
.personal .tags .tag-box {width: 80%;}
.personal .company {padding: 1.07rem 0 0 1.21rem;}
.personal .company .icon {width: 2.142rem;height: 2.142rem;border-radius: 2rem;}

/*股东高管*/
.management .nav {width: 64%;}
.management .nav li {width: 49%;float: left;}
.management .nav li:nth-child(2) {float: right;}
.management .nav li a {width: 54.2%;margin: 0 auto;}
.management .portrait img {width: 2.64rem;border-radius: 2rem;}
.management .list .bord-b {padding: 1.07rem 1.21rem;}
.management .tags {margin: .1rem 0 .2rem 0;}
.management .tags > div {padding: .21rem;line-height: 1;border-radius: .15rem;background: #DFE1E6;}

/*工商信息*/
.businessInfo .nav li {width: 24.5%;float: left;}
.businessInfo .nav li a {width: 83.4%;margin: 0 auto;}
.businessInfo .registerInfo {min-height: 43.4rem;}
.businessInfo .registerInfo {padding: 1.2rem 1.214rem;}
.businessInfo .registerInfo table td {border: none;padding: 0;padding-bottom: 1rem;}
.businessInfo .changeRec {min-height: 42.8rem;}
.businessInfo .changeRec {padding: 1.21rem;padding-right: 0;}
.businessInfo .changeRec li {border-color: #e4e4e4;}
.businessInfo .changeRec li:before {content: '';display: inline-block;width: .85rem;height: 1.1rem;background: transparent url(/Public/wechat/images/company/circle.png) no-repeat center bottom;background-size:100% .85rem;position: absolute;left: -.5rem;}
.businessInfo .changeRec li:first-child:before {background: #fff url(/Public/wechat/images/company/circle.png) no-repeat center bottom;background-size:100% .85rem;}
.businessInfo .changeRec li .bord-b {border-color: #eaeaea;}
.businessInfo .changeRec li .before,.businessInfo .changeRec li .after {width: 45%;word-break: break-all;}
.businessInfo .changeRec li .before {width: 45%;margin-right: 2rem;}
.businessInfo .investment .list a,.businessInfo .annualRep .list a {padding: 1.07rem 1.21rem;}
.businessInfo .investment .list .content {width: 84%;}
.businessInfo .investment .portrait img{width: 3.28rem;height: 3.28rem;border-radius: .2rem;}
.businessInfo .investment .col-xs-4 {float:left;width: 33.3333%;}
.businessInfo .annualRep .list a {line-height: 1;}
.businessInfo .annualRep .list a:after {content: '';display: block;width: .6rem;height: .96rem;background: url(/Public/wechat/images/company/arrow-r.png) no-repeat;background-size: 100% 100%;position: absolute;right: 1.21rem;top: 1.07rem;}

/*年报详情*/
.report .box .hd span {display:block;width:.6rem;height:.96rem;background: url(/Public/wechat/images/company/arrow-r.png) no-repeat;background-size: 100% 100%;position: absolute;right: 1.071rem;top: 1.21rem;transition: all .5s;}
.report .box .hd.transform span {transform: rotate(90deg);}
.report .box .bd > .content {margin-bottom: 1.428rem;}
.report .box .bd > .content:last-child {margin-bottom: 0}
.report .box .table td {border: none;padding: 0;padding-bottom: 1.428rem;}

/*知识产权*/
.property .nav {width: 64%;}
.property .nav li {width: 49%;float: left;}
.property .nav li:nth-child(2) {float: right;}
.property .nav li a {width: 64%;margin: 0 auto;}
.property .portrait img {width: 3.28rem;height: 3.28rem;border: 1px solid #eaeaea;border-radius: .2rem;}
.property .list .bord-b {padding: 1.07rem 1.21rem;position: relative;}
.property #patent .list .bord-b:after {content: '';display: block;width: .6rem;height: .96rem;background: url(/Public/wechat/images/company/arrow-r.png) no-repeat;background-size: 100% 100%;position: absolute;right: 1.21rem;top: 2.6rem;}
.property #trademark .content {width: 82%;}
.property #trademark .name {width:100%;overflow:hidden;white-space: nowrap;text-overflow: ellipsis;}

/*信用信息*/
.creditInfo .nav {width: 64%;}
.creditInfo .nav li {width: 49%;float: left;}
.creditInfo .nav li:nth-child(2) {float: right;}
.creditInfo .nav li a {width: 64%;margin: 0 auto;}
.creditInfo .list .bord-b {padding: 1.07rem 1.21rem;}
.creditInfo .list table td {border: none;padding: 0;padding-bottom: 1.071rem;}
.creditInfo .list table tr:last-child td {padding-bottom: 0;}

/*邀请好友app内页*/
.invite .banner {width: 100%;height: 30.64rem;background: url(/Public/wechat/images/user/bg-01.png) no-repeat;background-size: 100% 100%}
.invite .methods {width: 100%;height: 13.28rem;background: url(/Public/wechat/images/user/bg-05.png) no-repeat;background-size: 100% 100%}
.invite .methods .content {width: 93%;height: 13.428rem;background: url(/Public/wechat/images/user/bg-02.png) no-repeat;background-size: 100% 100%;margin: 0 auto;padding-top: 1.8rem;}
.invite .methods .content .clearfix {padding: 1.6rem 1.4rem 0;}
.invite .methods h2 {color: #fe5c11;font-size: 1.36rem;width:10.25rem;height:1.428rem;line-height:1.428rem;background: url(/Public/wechat/images/user/title.png) no-repeat center center;background-size: 100% .285rem;margin: 0 auto;}
.invite .methods .item {width: 25%;padding-top: 4.2rem;color: #151515;}
.invite .methods .item.wechat {background: url(/Public/wechat/images/user/icon-01.png) no-repeat center top;background-size: 3.78rem 3.78rem;}
.invite .methods .item.moments {background: url(/Public/wechat/images/user/icon-02.png) no-repeat center top;background-size: 3.78rem 3.78rem;}
.invite .methods .item.qq {background: url(/Public/wechat/images/user/icon-03.png) no-repeat center top;background-size: 3.78rem 3.78rem;}
.invite .methods .item.link {background: url(/Public/wechat/images/user/icon-04.png) no-repeat center top;background-size: 3.78rem 3.78rem;}
.invite .record {background: #ffaf2f;color:#fe5c11;padding-top: 1.8rem;padding-bottom: 1.82rem;}
.invite .record .title {width: 93%;height: 4.32rem;padding:2rem 2rem 0;background: url(/Public/wechat/images/user/bg-04.png) no-repeat;background-size: 100% 100%;margin: 0 auto;}
.invite .record .topLoop {width: 87.6%;background: #fff;margin: 0 auto;box-shadow: #fff1e7 0px 0px 2.6rem inset;border-bottom-left-radius: 1rem;border-bottom-right-radius: 1rem;}
.invite .record .topLoop .bd {height: 100%;overflow: hidden;border-bottom-left-radius: 1rem;border-bottom-right-radius: 1rem;}
.invite .record .topLoop ul li {height: 2.07rem;line-height: 2.07rem;}
.invite .record .topLoop ul li:nth-of-type(odd){background: #fff1e7;}
.invite .record ul li .fl,.invite .record ul li .fr {width: 50%;}

/*邀请好友微信注册页*/
.register .banner {width: 100%;height: 16.7rem;background: url(/Public/wechat/images/user/bg-06.png) no-repeat;background-size: 100% 100%;padding-top: 13rem;}
.register .banner .invite {width: 17.28rem;height: 2.428rem;line-height:2.2rem;background: url(/Public/wechat/images/user/bg-08.png) no-repeat;background-size: 100% 100%;margin: 0 auto;}
.register .content {background: url(/Public/wechat/images/user/icon-05.png) no-repeat right 15.2rem;background-size: 4.25rem 8.17rem;}
.register .btn-primary {font-size:1.28rem;width: 100%;height: 3.35rem;line-height: 3.35rem;border-radius: 3rem;background: -webkit-gradient(linear, left top, right bottom, color-stop(50%, #ff8423), color-stop(100%, #ffb643));background: -webkit-linear-gradient(left, #ff8423 50%, #ffb643 100%);background: linear-gradient(to right, #ff8423 0%, #ffb643 100%);margin: 2.14rem auto 0;}
.register .reg-form {width: 100%;padding: 0 1.21rem;background: url(/Public/wechat/images/user/bg-07.png) no-repeat center top;background-size: 100% 7.03rem;}
.register .reg-form .bg-white {width: 100%;padding: 2.78rem 1.78rem;border-radius: .8rem;}
.register .reg-form .form-group .form-control {font-size:1.0769rem;border:none;border:1px solid #dfe1e6;border-radius: 3rem;overflow: hidden;height: 3.35rem;padding: .2rem 1.07rem;}
.register .reg-form .form-group .form-control:focus {border-color: #66afe9;outline: 0;-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6);box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px rgba(102, 175, 233, .6)}
.register input::-webkit-input-placeholder {color: #dfe1e6;font-size: 1.07rem;}
.register .reg-form .form-group .yzm {color: #ff7e2b;font-size: 1.07rem;right: 1rem;top: .8rem;}
.register .reg-form .btn-primary {margin-top: 1.428rem;width:100%;}
.recommend {padding: 2.14rem 1.21rem 1.57rem;background: url(/Public/wechat/images/user/icon-06.png) no-repeat 0 9rem;background-size: 6.75rem 10.6rem;}
.recommend .bg-white {border-radius: .8rem;padding: 0 1.21rem 2.14rem;}
.recommend .bg-white .clearfix {border-bottom: 1px dashed #eaeaea;height: 9.28rem;}
.recommend .bg-white .clearfix:last-child {border-bottom: none;}
.recommend .text {color: #303030;}
.recommend .text1 {width: 42%;}
.recommend .text2 {width: 46%;}
.recommend .text3 {width: 42%;}
.recommend .icon01 {width: 10.57rem;height: 5.1rem;background: url(/Public/wechat/images/user/icon-08.png) no-repeat;background-size: 100% 100%;}
.recommend .icon02 {width: 9rem;height: 6.92rem;background: url(/Public/wechat/images/user/icon-10.png) no-repeat;background-size: 100% 100%;}
.recommend .icon03 {width: 9.28rem;height: 6.57rem;background: url(/Public/wechat/images/user/icon-09.png) no-repeat;background-size: 100% 100%;}
.regsucceed .banner {width: 100%;height: 30.92rem;background: url(/Public/wechat/images/user/bg-09.png) no-repeat;background-size: 100% 100%;padding-top: 13rem;}
.regsucceed .banner .toApp {width: 8.78rem;height: 2.92rem;line-height:2.92rem;border-radius: 3rem;background: #ffac97;border: 1px solid #fff;margin: 0 auto;}
.regsucceed .content {margin-top: -6rem;}
.regsucceed .recommend {background: url(/Public/wechat/images/user/icon-06.png) no-repeat 0 32rem;background-size: 6.75rem 10.6rem;}
.regsucceed .content {background: url(/Public/wechat/images/user/icon-05.png) no-repeat right 16.8rem;background-size: 4.25rem 8.17rem;}

/*举报页面*/
.tipsOff .container {padding: 0 1.21rem;}
.tipsOff .weui-cell__bd {font-size: 1.07rem;}
.tipsOff .weui-cell {padding: .71rem 1.21rem;}
.tipsOff .weui-cells_checkbox .weui-icon-checked:before {font-size: 1.07rem;}
.tipsOff .weui-cells_checkbox .weui-check:checked+.weui-icon-checked:before {color: #527AF7;}
.tipsOff .weui-check__label:active {background: transparent;}
.tipsOff .fillText .fl {width: 70.6%;}
.tipsOff .fillText .fl textarea {width: 100%;height: 4.857rem;border: none;outline: none;}
.tipsOff .fillText .fl textarea:focus {border: none;outline: none;}
.tipsOff .fillText .fr {width: 4.857rem;height: 4.857rem;border: 1px solid #d7d7d7;border-radius: .21rem;}
.tipsOff .fillText .fr .trigger {width: 4.857rem;height: 4.857rem;background: url(/Public/wechat/images/user/icon-camera.png) no-repeat center .85rem ;background-size: 1.714rem 1.714rem;padding-top: 2.6rem;}
.tipsOff .img-upload {display: none;}
.tipsOff .btn {color:#ffffff;font-size: 1.28rem;width:90%;margin:3.14rem auto 0;padding:0;border-radius: .35rem;height:3.2rem;line-height:3.2rem;background: -webkit-gradient(linear, left top, right bottom, color-stop(50%, #477BF6), color-stop(100%, #3D8CF7));background: -webkit-linear-gradient(left, #477BF6 50%, #3D8CF7 100%);background: linear-gradient(to right, #477BF6 0%, #3D8CF7 100%);}

/*状态*/
.nodata {width: 100%;height: 10rem;background: url(/Public/wechat/images/company/nodata.png) no-repeat center center;background-size: 6.7rem 7.32rem;padding-top: 8.2rem;margin-top: 12rem;}

/* for 760 px width screen */
@media only screen and (max-width:768px) { html, body { font-size: 16px; font-family:"微软雅黑";min-height: 700px;} }
/* for 640 px width screen */
@media only screen and (max-width:640px) { html, body { font-size: 15px; } }
/* for 540 px width screen */
@media only screen and (max-width:540px) { html, body { font-size: 15px;  } }
/* for 480 px width screen */
@media only screen and (max-width: 480px) { html, body { font-size: 15px;min-height: 736px;  } }
/* for 400 px width screen */
@media only screen and (max-width: 410px) { html, body {  font-size: 14px;min-height: 667px;  } }
/* for 360 px width screen */
@media only screen and (max-width: 360px) { html, body {  font-size: 14px;min-height: 640px;  } }
/* for 320 px width screen */
@media only screen and (max-width: 320px) { html, body {  font-size: 12px;min-height: 568px; } }
/* for 240 px width screen */
@media only screen and (max-width: 240px) { html, body {  font-size: 12px; } }