/* 弹出控件 */
.qhz-layer-shade, .qhz-layer{position:fixed; _position:absolute; pointer-events: auto;}
.qhz-layer-shade{top:0; left:0; width:100%; height:100%; _height:expression(document.body.offsetHeight+"px");}
.qhz-layer{top:150px; left: 0; margin:0; padding:0; background-color:#fff; -webkit-background-clip: content; border: 9px solid rgba(0,0,0,.25);}
.qhz-layer-close{position:absolute;}
.qhz-layer-content{position:relative;}
.qhz-layer-border{border:1px solid #B2B2B2; border:1px solid rgba(0,0,0,.3); box-shadow: 1px 1px 5px rgba(0,0,0,.2);}
.qhz-layer-moves{position:absolute; border:3px solid #666; border:3px solid rgba(0,0,0,.5); cursor:move; background-color:#fff; background-color:rgba(255,255,255,.3); filter:alpha(opacity=50);}
.qhz-layer-load{background:url(../images/loading-0.gif) #fff center center no-repeat;}
.qhz-layer-ico{ background:url(../images/icon.png) no-repeat;}
.qhz-layer-dialog .qhz-layer-ico,
.qhz-layer-setwin a,
.qhz-layer-btn a{display:inline-block; *display:inline; *zoom:1; vertical-align:top;}

/* 动画 */
.qhz-layer{border-radius: 2px; -webkit-animation-fill-mode: both; animation-fill-mode: both; -webkit-animation-duration:.3s; animation-duration:.3s;}

@-webkit-keyframes bounceIn { /* 默认 */
    0% {
        opacity: 0;
        -webkit-transform: scale(.5);
        transform: scale(.5)
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        transform: scale(1)
    }
}
@keyframes bounceIn {
    0% {
        opacity: 0;
        -webkit-transform: scale(.5);
        -ms-transform: scale(.5);
        transform: scale(.5)
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        -ms-transform: scale(1);
        transform: scale(1)
    }
}
.layer-anim{-webkit-animation-name: bounceIn;animation-name: bounceIn}
@-webkit-keyframes bounceOut {
    100% {
        opacity: 0;
        -webkit-transform: scale(.7);
        transform: scale(.7)
    }
    30% {
        -webkit-transform: scale(1.03);
        transform: scale(1.03)
    }
    0% {
        -webkit-transform: scale(1);
        transform: scale(1);
    }
}
@keyframes bounceOut {
    100% {
        opacity: 0;
        -webkit-transform: scale(.7);
        -ms-transform: scale(.7);
        transform: scale(.7)
    }
    30% {
        -webkit-transform: scale(1.03);
        -ms-transform: scale(1.03);
        transform: scale(1.03)
    }
    0% {
        -webkit-transform: scale(1);
        -ms-transform: scale(1);
        transform: scale(1)
    }
}
.layer-anim-close{-webkit-animation-name: bounceOut;animation-name: bounceOut; -webkit-animation-duration:.2s; animation-duration:.2s;}

@-webkit-keyframes zoomInDown{0%{opacity:0;-webkit-transform:scale(.1) translateY(-2000px);transform:scale(.1) translateY(-2000px);-webkit-animation-timing-function:ease-in-out;animation-timing-function:ease-in-out}60%{opacity:1;-webkit-transform:scale(.475) translateY(60px);transform:scale(.475) translateY(60px);-webkit-animation-timing-function:ease-out;animation-timing-function:ease-out}}@keyframes zoomInDown{0%{opacity:0;-webkit-transform:scale(.1) translateY(-2000px);-ms-transform:scale(.1) translateY(-2000px);transform:scale(.1) translateY(-2000px);-webkit-animation-timing-function:ease-in-out;animation-timing-function:ease-in-out}60%{opacity:1;-webkit-transform:scale(.475) translateY(60px);-ms-transform:scale(.475) translateY(60px);transform:scale(.475) translateY(60px);-webkit-animation-timing-function:ease-out;animation-timing-function:ease-out}}.layer-anim-01{-webkit-animation-name:zoomInDown;animation-name:zoomInDown}

@-webkit-keyframes fadeInUpBig{0%{opacity:0;-webkit-transform:translateY(2000px);transform:translateY(2000px)}100%{opacity:1;-webkit-transform:translateY(0);transform:translateY(0)}}@keyframes fadeInUpBig{0%{opacity:0;-webkit-transform:translateY(2000px);-ms-transform:translateY(2000px);transform:translateY(2000px)}100%{opacity:1;-webkit-transform:translateY(0);-ms-transform:translateY(0);transform:translateY(0)}}.layer-anim-02{-webkit-animation-name:fadeInUpBig;animation-name:fadeInUpBig}

@-webkit-keyframes zoomInLeft{0%{opacity:0;-webkit-transform:scale(.1) translateX(-2000px);transform:scale(.1) translateX(-2000px);-webkit-animation-timing-function:ease-in-out;animation-timing-function:ease-in-out}60%{opacity:1;-webkit-transform:scale(.475) translateX(48px);transform:scale(.475) translateX(48px);-webkit-animation-timing-function:ease-out;animation-timing-function:ease-out}}@keyframes zoomInLeft{0%{opacity:0;-webkit-transform:scale(.1) translateX(-2000px);-ms-transform:scale(.1) translateX(-2000px);transform:scale(.1) translateX(-2000px);-webkit-animation-timing-function:ease-in-out;animation-timing-function:ease-in-out}60%{opacity:1;-webkit-transform:scale(.475) translateX(48px);-ms-transform:scale(.475) translateX(48px);transform:scale(.475) translateX(48px);-webkit-animation-timing-function:ease-out;animation-timing-function:ease-out}}.layer-anim-03{-webkit-animation-name:zoomInLeft;animation-name:zoomInLeft}

@-webkit-keyframes rollIn{0%{opacity:0;-webkit-transform:translateX(-100%) rotate(-120deg);transform:translateX(-100%) rotate(-120deg)}100%{opacity:1;-webkit-transform:translateX(0px) rotate(0deg);transform:translateX(0px) rotate(0deg)}}@keyframes rollIn{0%{opacity:0;-webkit-transform:translateX(-100%) rotate(-120deg);-ms-transform:translateX(-100%) rotate(-120deg);transform:translateX(-100%) rotate(-120deg)}100%{opacity:1;-webkit-transform:translateX(0px) rotate(0deg);-ms-transform:translateX(0px) rotate(0deg);transform:translateX(0px) rotate(0deg)}}.layer-anim-04{-webkit-animation-name:rollIn;animation-name:rollIn}

@keyframes fadeIn{0%{opacity:0}100%{opacity:1}}.layer-anim-05{-webkit-animation-name:fadeIn;animation-name:fadeIn}

@-webkit-keyframes shake{0%,100%{-webkit-transform:translateX(0);transform:translateX(0)}10%,30%,50%,70%,90%{-webkit-transform:translateX(-10px);transform:translateX(-10px)}20%,40%,60%,80%{-webkit-transform:translateX(10px);transform:translateX(10px)}}@keyframes shake{0%,100%{-webkit-transform:translateX(0);-ms-transform:translateX(0);transform:translateX(0)}10%,30%,50%,70%,90%{-webkit-transform:translateX(-10px);-ms-transform:translateX(-10px);transform:translateX(-10px)}20%,40%,60%,80%{-webkit-transform:translateX(10px);-ms-transform:translateX(10px);transform:translateX(10px)}}.layer-anim-06{-webkit-animation-name:shake;animation-name:shake}@-webkit-keyframes fadeIn{0%{opacity:0}100%{opacity:1}}

/* 标题栏 */
.qhz-layer-title{padding:0 80px 0 20px; height:42px; line-height:42px; border-bottom:1px solid #eee; font-size:14px; color:#333; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; background-color: #F8F8F8; border-radius: 2px 2px 0 0;}
.qhz-layer-setwin{position:absolute; right:15px; *right:0; top:15px; font-size:0; line-height: initial;}
.qhz-layer-setwin a{position:relative; width: 16px; height:16px; margin-left:10px; font-size:12px; _overflow:hidden;}
.qhz-layer-setwin .qhz-layer-close1{background-position:0 -40px; cursor:pointer;}
.qhz-layer-setwin .qhz-layer-close1:hover{opacity:0.7;}
.qhz-layer-setwin .qhz-layer-close2{position:absolute; right:-28px; top:-28px; width:30px; height:30px;  margin-left:0; background-position:-149px -31px; *right:-18px; _display:none;}
.qhz-layer-setwin .qhz-layer-close2:hover{ background-position:-180px -31px;}

/* 按钮栏 */
.qhz-layer-btn{text-align: center; padding:0 10px 12px; pointer-events: auto;}
.qhz-layer-btn a{height: 28px; line-height: 28px; margin: 0 6px; padding: 0 15px; border:1px #dedede solid; background-color:#f1f1f1; color: #333; border-radius: 2px; font-weight:400; cursor:pointer; text-decoration: none;}
.qhz-layer-btn a:hover{opacity:0.9; text-decoration: none;}
.qhz-layer-btn a:active{opacity:0.7;}
.qhz-layer-btn .qhz-layer-btn0{border: none; background-color: #f47d30; color:#fff;}

/* 定制化 */
.qhz-layer-dialog{min-width:260px;}
.qhz-layer-dialog .qhz-layer-content{position: relative; padding:20px; line-height:24px; word-break:break-all; overflow:hidden; font-size:14px; overflow:auto;}
.qhz-layer-dialog .qhz-layer-content .qhz-layer-ico{position:absolute; top:16px; left:15px; _left:-40px; width:30px; height:30px;}
.qhz-layer-ico1{background-position:-30px 0 }
.qhz-layer-ico2{background-position:-60px 0;}
.qhz-layer-ico3{background-position:-90px 0;}
.qhz-layer-ico4{background-position:-120px 0;}
.qhz-layer-ico5{background-position:-150px 0;}
.qhz-layer-ico6{background-position:-180px 0;}
.qhz-layer-rim{border:6px solid #8D8D8D; border:6px solid rgba(0,0,0,.3); border-radius:5px; box-shadow: none;}
.qhz-layer-msg{min-width:180px; border:1px solid #D3D4D3; box-shadow: none;}
.qhz-layer-hui{min-width:100px;  background-color:#000; filter:alpha(opacity=60); background-color: rgba(0,0,0,0.6); color: #fff; border:none;}
.qhz-layer-hui .qhz-layer-content{padding:12px 25px; text-align:center;}
.qhz-layer-dialog .qhz-layer-padding{padding: 20px 20px 20px 55px; text-align: left;}
.qhz-layer-page .qhz-layer-content{position:relative; overflow:auto;}
.qhz-layer-page .qhz-layer-btn,.qhz-layer-iframe .qhz-layer-btn{padding-top:10px;}
.qhz-layer-nobg{background:none;}
.qhz-layer-iframe .qhz-layer-content{overflow:hidden;}
.qhz-layer-iframe iframe{display:block; width:100%;}
.qhz-layer-loading{border-radius:100%; background:none;  box-shadow:none;  border:none;}
.qhz-layer-loading .qhz-layer-content{width:60px; height:24px; background:url(../images/loading-0.gif) no-repeat;}
.qhz-layer-loading .qhz-layer-loading1{width:37px; height:37px; background:url(../images/loading-1.gif) no-repeat;}
.qhz-layer-loading .qhz-layer-loading2, .qhz-layer-ico16{width:32px; height:32px; background:url(../images/loading-2.gif) no-repeat;}
.qhz-layer-tips{background:none; box-shadow:none; border:none;}
.qhz-layer-tips .qhz-layer-content{position:relative; line-height:22px; min-width: 12px; padding:5px 10px; font-size:12px; _float:left; border-radius:3px; box-shadow: 1px 1px 3px rgba(0,0,0,.3); background-color:#FF9900; color:#fff;}
.qhz-layer-tips .qhz-layer-close{right:-2px; top:-1px;}
.qhz-layer-tips i.qhz-layer-TipsG{ position:absolute;  width:0; height:0; border-width:8px; border-color:transparent; border-style:dashed; *overflow:hidden;}
.qhz-layer-tips i.qhz-layer-TipsT, .qhz-layer-tips i.qhz-layer-TipsB{left:5px; border-right-style:solid; border-right-color:#FF9900;}
.qhz-layer-tips i.qhz-layer-TipsT{bottom:-8px;}
.qhz-layer-tips i.qhz-layer-TipsB{top:-8px;}
.qhz-layer-tips i.qhz-layer-TipsR, .qhz-layer-tips i.qhz-layer-TipsL{top:1px; border-bottom-style:solid; border-bottom-color:#FF9900;}
.qhz-layer-tips i.qhz-layer-TipsR{left:-8px;}
.qhz-layer-tips i.qhz-layer-TipsL{right:-8px;}
.qhz-layer-iconext{background:url(../images/icon-ext.png) no-repeat;}
.qhz-layer-prompt .qhz-layer-input{display:block; width:220px; height:30px; margin:0 auto; line-height:30px; padding: 0 5px; border: 1px solid #ccc; box-shadow: 1px 1px 5px rgba(0,0,0,.1) inset; color:#333;}
.qhz-layer-prompt textarea.qhz-layer-input{width:300px; height:100px; line-height:20px;}
/* 弹出控件 END*/